{
  "extends": [
    "eslint:recommended",
    "plugin:import/errors",
    "plugin:import/warnings",
    "plugin:jsx-a11y/recommended",
    "plugin:prettier/recommended"
  ],
  "env": {
    "browser": true,
    "es6": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true,
      "experimentalObjectRestSpread": true
    },
    "sourceType": "module"
  },
  "plugins": ["react", "jsx-a11y", "import", "prettier"],
  "rules": {
    "no-undefined": 1,
    "no-unused-vars": 1,
    "react/jsx-uses-react": 1,
    "react/prop-types": 1,
    "react/jsx-uses-vars": 1,
    "react/react-in-jsx-scope": 1,
    "import/no-unresolved": 0,
    "import/namespace": 2,
    "import/default": 2,
    "import/export": 2,
    "import/no-named-as-default": 0,
    "no-console": 0,
    "prettier/prettier": [
      "error",
      {
        "singleQuote": true
      }
    ]
  },
  "globals": {
    "test": false,
    "global": false,
    "expect": false,
    "beforeAll": false,
    "afterAll": false,
    "beforeEach": false,
    "afterEach": false,
    "require": false,
    "__dirname": false,
    "module": false,
    "exports": false,
    "describe": false,
    "it": false
  },
  "settings": {
    "import/resolver": "webpack"
  }
}
